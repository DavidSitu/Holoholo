//Should be silver book instead

#include <iostream>
#include <string>
#include "GoldBook.h"
#include "GoldCheckpoint.h"

int check_book(const GoldBook& goldBook) {
    for (const auto& checkpoint : goldBook.getCheckpoints()) {
        if (!checkpoint.getIfUnlocked()) {
            return 0;
        }
    }
    return 150;
}
